{
    "compilerOptions": {
        "lib": ["ESNext", "DOM"],
        "target": "ESNext",
        "module": "ESNext",
        "moduleResolution": "bundler",
        "allowSyntheticDefaultImports": true,
        "esModuleInterop": true,
        "jsx": "preserve",
        "jsxImportSource": "solid-js",
        "allowJs": true,
        "strict": true,
        "skipLibCheck": true,
        "noEmit": true,
        "types": ["vinxi/types/client"],
        "isolatedModules": true,
        "baseUrl": ".",
        "paths": {
            "@/*": ["./src/*"],
            "@styled-system/*": ["./styled-system/*"]
        }
    }
}
